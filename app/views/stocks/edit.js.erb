    this.value = this.value.toUpperCase();
    this.value = this.value.replace(/^\s+|\s+$/g,'');
});
$('#stock_quantity').keyup(function(){
    this.value = this.value.toUpperCase();
    this.value = this.value.replace(/^\s+|\s+$/g,'');
});









